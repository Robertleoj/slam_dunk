// monster.fbs
namespace slamd.flatb;

enum TreeType: byte {
    NONE = 0,
    CANVAS = 1,
    SCENE = 2
}

table Tree {
    id: ulong;
    tree_type: TreeType;
}

table View {
    name: string;
    tree_id: ulong;
}

table InitialState {
    name: string;
    views: [View];
    trees: [Tree];
}

root_type InitialState;