// monster.fbs
namespace slamd.flatb;



table Tree {
    id: ulong;
}

enum ViewType: byte {
    NONE = 0,
    CANVAS = 1,
    SCENE = 2
}

table View {
    name: string;
    tree_id: ulong;
    view_type: ViewType;
}

table InitialState {
    name: string;
    views: [View];
    trees: [Tree];
}

root_type InitialState;